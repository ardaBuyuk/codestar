<%- include('partials/header') %>
 <section>
     <div class="html-posts">
         <h2 class="text-light">JavaScript (NodeJS) Paylaşımları</h2>
         <h2 class="text-light for-mobile">JS</h2>
         <input type="text" name="search" id="search" placeholder="Ara..." class = "form-control">
     </div>
     <div class="posts">
         <!-- Card -->
             <!-- Card -->
             <% data.forEach((d) => { %>
              <div class="card" style="width: 18rem;">
               <div class="card-body">
                 <h5 class="card-title"><%= d.title %></h5>
                 <p class="card-text"><%= d.description %></p>
                 <a href="/javascript/<%= d._id %>" class="btn btn-dark">Kodu Aç</a>
               </div>
              </div>
              <% }); %>
<!-- Card Code End  -->
     </div>
 </section>
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js%22%3E"></script>
   <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="js/bootstrap.bundle.js"></script>
<script src="js/bootstrap.bundle.min.js"></script>
<script>
  $(document).ready(function(){
   $("#search").keyup(function(){
     var value = $(this).val().toLowerCase();
     $(".card, .wrapper").filter(function() {
       $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
     });
   });
 });
</script>
</body>
</html>